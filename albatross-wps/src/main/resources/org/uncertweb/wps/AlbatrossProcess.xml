<?xml version="1.0" encoding="UTF-8"?>
<wps:ProcessDescriptions xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opengis.net/wps/1.0.0
http://schemas.opengis.net/wps/1.0.0/wpsDescribeProcess_response.xsd" service="WPS" version="1.0.0" xml:lang="en-US">
	<ProcessDescription wps:processVersion="2" storeSupported="true" statusSupported="false">
		<ows:Identifier>org.uncertweb.wps.AlbatrossProcess</ows:Identifier>
		<ows:Title>Albatross</ows:Title>
		<ows:Abstract>
			Uses a part of the Albatross Model to compute the output
		</ows:Abstract>
		<ows:Metadata xlink:title="model"/>
		<DataInputs>
		<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>genpop-households</ows:Identifier>
				<ows:Title>Number of households created by Genpop and stored in “test.obs”</ows:Title>
				<ows:Abstract></ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer"></ows:DataType>
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>rwdata-households</ows:Identifier>
				<ows:Title>Number of household activity schedules sets created by Rwdata and stored in “test.prd”</ows:Title>
				<ows:Abstract></ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer"></ows:DataType>
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>postcode-areas</ows:Identifier>
				<ows:Title>Number of postcode areas (PC4)</ows:Title>
				<ows:Abstract></ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer"></ows:DataType>
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>zones</ows:Identifier>
				<ows:Title>Number of zones (larger areas)</ows:Title>
				<ows:Abstract></ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer"></ows:DataType>
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>municipalities</ows:Identifier>
				<ows:Title>Number of municipalities</ows:Title>
				<ows:Abstract></ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer"></ows:DataType>
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>isModelUncertainty</ows:Identifier>
				<ows:Title>Indicates whether model uncertainty (true) should be used or just input uncertainty (false).</ows:Title>
				<ows:Abstract></ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:boolean"></ows:DataType>
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>export-file</ows:Identifier>
				<ows:Title>exportfile generated by synPop</ows:Title>
				<ows:Abstract></ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:string"></ows:DataType>
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>export-file-bin</ows:Identifier>
				<ows:Title>bin exportfile generated by synPop</ows:Title>
				<ows:Abstract></ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:string"></ows:DataType>
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="100">
				<ows:Identifier>uncert-link</ows:Identifier>
				<ows:Title>link or group of links with standard deviation</ows:Title>
				<ows:Abstract></ows:Abstract>
				<ComplexData>
					<Default>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>
								http://52north.org/schema/geostatistics/uncertweb/Profiles/Albatross/albatross_uInput.xsd
							</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>
								http://52north.org/schema/geostatistics/uncertweb/Profiles/Albatross/albatross_uInput.xsd
							</Schema>
						</Format>
					</Supported>
				</ComplexData>
			</Input>
			<Input minOccurs="0" maxOccurs="100">
				<ows:Identifier>uncert-area</ows:Identifier>
				<ows:Title>area or group of areas with standard deviation for sector; sector is implemented as parameter with name attribute value 'sector'</ows:Title>
				<ows:Abstract></ows:Abstract>
				<ComplexData>
					<Default>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>
								http://52north.org/schema/geostatistics/uncertweb/Profiles/Albatross/albatross_uInput.xsd
							</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>
								http://52north.org/schema/geostatistics/uncertweb/Profiles/Albatross/albatross_uInput.xsd
							</Schema>
						</Format>
					</Supported>
				</ComplexData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>number-of-schedules</ows:Identifier>
				<ows:Title>Number of schedules returned in case the requested output is om_schedules.</ows:Title>
				<ows:Abstract></ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer"></ows:DataType>
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
		</DataInputs>
		<ProcessOutputs>
			<Output>
				<ows:Identifier>om_ODmatrix</ows:Identifier>
				<ows:Title>om_ODmatrix</ows:Title>
				<ows:Abstract>om_ODmatrix</ows:Abstract>
				<ComplexOutput>
					<Default>
						<Format>
							<MimeType>application/x-om-u+xml</MimeType>
							<Schema>http://52north.org/schema/geostatistics/uncertweb/Profiles/OM/UncertWeb_OM.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>application/x-om-u+json</MimeType>
							<Schema>http://52north.org/schema/geostatistics/uncertweb/Profiles/OM/UncertWeb_OM.xsd</Schema>
						</Format>
					</Supported>
				</ComplexOutput>
			</Output>
			<Output>
				<ows:Identifier>om_indicators</ows:Identifier>
				<ows:Title>om_indicators</ows:Title>
				<ows:Abstract>om_indicators</ows:Abstract>
				<ComplexOutput>
					<Default>
						<Format>
							<MimeType>application/x-om-u+xml</MimeType>
							<Schema>http://52north.org/schema/geostatistics/uncertweb/Profiles/OM/UncertWeb_OM.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>application/x-om-u+json</MimeType>
							<Schema>http://52north.org/schema/geostatistics/uncertweb/Profiles/OM/UncertWeb_OM.xsd</Schema>
						</Format>
					</Supported>
				</ComplexOutput>
			</Output>
			<Output>
				<ows:Identifier>om_schedules</ows:Identifier>
				<ows:Title>om_schedules</ows:Title>
				<ows:Abstract>om_schedules</ows:Abstract>
				<ComplexOutput>
					<Default>
						<Format>
							<MimeType>application/x-om-u+xml</MimeType>
							<Schema>http://52north.org/schema/geostatistics/uncertweb/Profiles/OM/UncertWeb_OM.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>application/x-om-u+json</MimeType>
							<Schema>http://52north.org/schema/geostatistics/uncertweb/Profiles/OM/UncertWeb_OM.xsd</Schema>
						</Format>
					</Supported>
				</ComplexOutput>
			</Output>
			<Output>
				<ows:Identifier>export-file</ows:Identifier>
				<ows:Title>Export File</ows:Title>
				<ows:Abstract>Raw plain text export file generated by Albatross.</ows:Abstract>
				<ComplexOutput>
					<Default>
						<Format>
							<MimeType>text/plain</MimeType>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/plain</MimeType>
						</Format>
					</Supported>
				</ComplexOutput>
			</Output>
		</ProcessOutputs>
	</ProcessDescription>
</wps:ProcessDescriptions>
