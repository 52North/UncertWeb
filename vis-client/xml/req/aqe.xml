<sos:GetObservation service="SOS" version="1.0.0"
	xmlns:sos="http://www.opengis.net/sos/1.0" xmlns:gml="http://www.opengis.net/gml"
	xmlns:ogc="http://www.opengis.net/ogc" xmlns:om="http://www.opengis.net/om/1.0">
	<sos:offering>O3</sos:offering>
	<sos:eventTime>
		<ogc:TM_Equals>
			<ogc:PropertyName>om:SamplingTime</ogc:PropertyName>
			<gml:TimePeriod>
				<gml:beginPosition>2001-01-01T00:00:00.000Z</gml:beginPosition>
				<gml:endPosition>2001-01-03T00:00:00.000Z</gml:endPosition>
			</gml:TimePeriod>
		</ogc:TM_Equals>
	</sos:eventTime>
	<sos:observedProperty>http://giv-genesis.uni-muenster.de:8080/SOR/REST/phenomenon/OGC/Concentration(O3)</sos:observedProperty>
	<sos:responseFormat>text/xml;subtype="om/1.0.0"</sos:responseFormat>
	<sos:resultModel>om:Observation</sos:resultModel>
	<sos:responseMode>inline</sos:responseMode>
</sos:GetObservation>
