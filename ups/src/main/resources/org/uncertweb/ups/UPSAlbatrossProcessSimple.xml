<?xml version="1.0" encoding="UTF-8"?>
<wps:ProcessDescriptions xml:lang="en-US"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" service="WPS" version="1.0.0"
	xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:wps="http://www.opengis.net/wps/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsDescribeProcess_response.xsd">
	<ProcessDescription wps:processVersion="1.0">
		<ows:Identifier>org.uncertweb.ups.UPSAlbatrossProcessSimple</ows:Identifier>
		<ows:Title />
		<DataInputs>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>AlbatrossServiceURL</ows:Identifier>
				<ows:Title></ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string" />
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>IdentifierAlbatrossProcess</ows:Identifier>
				<ows:Title></ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string" />
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>genpop-households</ows:Identifier>
				<ows:Title></ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string" />
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>rwdata-households</ows:Identifier>
				<ows:Title></ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string" />
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>postcode-areas</ows:Identifier>
				<ows:Title></ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string" />
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="9999">
				<ows:Identifier>zones</ows:Identifier>
				<ows:Title></ows:Title>
				<ows:Abstract>Input data with probabilistic uncertainty.</ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:string" />
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>municipalities</ows:Identifier>
				<ows:Title></ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string" />
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="100">
				<ows:Identifier>uncert-link</ows:Identifier>
				<ows:Title>link or group of links with standard deviation</ows:Title>
				<ows:Abstract></ows:Abstract>
				<ComplexData>
					<Default>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>
								http://v-mars.uni-muenster.de/uncertweb/schema/Profiles/Albatross/albatross_uInput.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>
								http://v-mars.uni-muenster.de/uncertweb/schema/Profiles/Albatross/albatross_uInput.xsd</Schema>
						</Format>
					</Supported>
				</ComplexData>
			</Input>
			<Input minOccurs="0" maxOccurs="100">
				<ows:Identifier>uncert-area</ows:Identifier>
				<ows:Title>area or group of areas with standard deviation for sector; sector is implemented as parameter with name attribute value 'sector'</ows:Title>
				<ows:Abstract></ows:Abstract>
				<ComplexData>
					<Default>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>
								http://v-mars.uni-muenster.de/uncertweb/schema/Profiles/Albatross/albatross_uInput.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>
								http://v-mars.uni-muenster.de/uncertweb/schema/Profiles/Albatross/albatross_uInput.xsd</Schema>
						</Format>
					</Supported>
				</ComplexData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>isModelUncertainty</ows:Identifier>
				<ows:Title>Indicates whether model uncertainty (true) should be used or just input uncertainty (false).</ows:Title>
				<ows:Abstract></ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:boolean"></ows:DataType>
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>export-file</ows:Identifier>
				<ows:Title>exportfile generated by synPop</ows:Title>
				<ows:Abstract></ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:string"></ows:DataType>
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>export-file-bin</ows:Identifier>
				<ows:Title>bin exportfile generated by synPop</ows:Title>
				<ows:Abstract></ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:string"></ows:DataType>
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>NumberOfRealisations</ows:Identifier>
				<ows:Title></ows:Title>
				<LiteralData>
				<ows:DataType ows:reference="xs:integer"/>
					<ows:AnyValue></ows:AnyValue>
				</LiteralData>
			</Input>
		</DataInputs>
		<ProcessOutputs>
			<Output>
				<ows:Identifier>om_schedules</ows:Identifier>
				<ows:Title>schedules</ows:Title>
				<ows:Abstract>schedules</ows:Abstract>
				<ComplexOutput>
					<Default>
						<Format>
							<MimeType>application/x-uncertml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>
								http://uncertml.org/uncertml.xsd
							</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>application/x-uncertml+xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>
								http://uncertml.org/uncertml.xsd
							</Schema>
						</Format>
					</Supported>
				</ComplexOutput>
			</Output>
			<Output>
				<ows:Identifier>ODmatrix</ows:Identifier>
				<ows:Title>ODmatrix</ows:Title>
				<ows:Abstract>ODmatrix</ows:Abstract>
				<ComplexOutput>
					<Default>
						<Format>
							<MimeType>application/x-om-u+xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/om/2.0/observation.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>application/x-om-u+xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/om/2.0/observation.xsd</Schema>
						</Format>
					</Supported>
				</ComplexOutput>
			</Output>
			<Output>
				<ows:Identifier>indicators</ows:Identifier>
				<ows:Title>indicators</ows:Title>
				<ows:Abstract>indicators</ows:Abstract>
				<ComplexOutput>
					<Default>
						<Format>
							<MimeType>application/x-om-u+xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/om/2.0/observation.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>application/x-om-u+xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/om/2.0/observation.xsd</Schema>
						</Format>
					</Supported>
				</ComplexOutput>
			</Output>
		</ProcessOutputs>
	</ProcessDescription>
</wps:ProcessDescriptions>
