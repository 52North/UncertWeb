<?xml version="1.0" encoding="UTF-8"?>
<!--This example describes .... -->
<wps:ProcessDescriptions xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opengis.net/wps/1.0.0
http://schemas.opengis.net/wps/1.0.0/wpsDescribeProcess_response.xsd" xml:lang="en-US" service="WPS" version="1.0.0">
	<ProcessDescription wps:processVersion="0" storeSupported="true" statusSupported="false">
		<ows:Identifier>org.uncertweb.wps.MultiGaussianDist2Realisations</ows:Identifier>
		<ows:Title>Distribution to realisation process.</ows:Title>
		<ows:Abstract>Computes a specified number of realisations for a given gaussian distribution.</ows:Abstract>
		<ows:Metadata xlink:title="spatial"/>
		<DataInputs>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>distribution</ows:Identifier>
				<ows:Title>distribution (netcdf works)</ows:Title>
				<ows:Abstract>distribution</ows:Abstract>
				<ComplexData>
					<Default>
						<Format>
							<MimeType>text/xml</MimeType>
							<Schema>http://giv-uw.uni-muenster.de:8080/uts/schemas/uncertml2.0.0/GaussianDistribution.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>application/netcdf</MimeType>
						</Format>
					</Supported>
				</ComplexData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>datasetNameMean</ows:Identifier>
				<ows:Title>datasetNameMean (for real)</ows:Title>
				<ows:Abstract>Name of the dataset for the mean values in the netcdf</ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:string"/>
					<ows:AllowedValues>
						<ows:Value/>
					</ows:AllowedValues>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>numbReal</ows:Identifier>
				<ows:Title>number of realisations (for real)</ows:Title>
				<ows:Abstract>number of realisations</ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer"/>
					<ows:AllowedValues>
						<ows:Value/>
					</ows:AllowedValues>
					<DefaultValue>10</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>variogram_model</ows:Identifier>
				<ows:Title>model (only for show)</ows:Title>
				<ows:Abstract>variogram model</ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer"/>
					<ows:AllowedValues>
						<ows:Value>Nug</ows:Value>
						<ows:Value>Exp</ows:Value>
						<ows:Value>Sph</ows:Value>
						<ows:Value>Gau</ows:Value>
						<ows:Value>Exc</ows:Value>
						<ows:Value>Mat</ows:Value>
						<ows:Value>Ste</ows:Value>
						<ows:Value>Cir</ows:Value>
						<ows:Value>Lin</ows:Value>
						<ows:Value>Bes</ows:Value>
						<ows:Value>Pen</ows:Value>
						<ows:Value>Per</ows:Value>
						<ows:Value>Hol</ows:Value>
						<ows:Value>Log</ows:Value>
						<ows:Value>Pow</ows:Value>
						<ows:Value>Spl</ows:Value>
						<ows:Value>Leg</ows:Value>
						<ows:Value>Err</ows:Value>
						<ows:Value>Int</ows:Value>
					</ows:AllowedValues>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>variogram_psill</ows:Identifier>
				<ows:Title>(partial) sill (only for show)</ows:Title>
				<ows:Abstract>(partial) sill of the variogram model component </ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer"/>
					<ows:AllowedValues>
						<ows:Value/>
					</ows:AllowedValues>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>variogram_range</ows:Identifier>
				<ows:Title>range (only for show)</ows:Title>
				<ows:Abstract>range of the variogram model component; in case of anisotropy: major range </ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer"/>
					<ows:AllowedValues>
						<ows:Value/>
					</ows:AllowedValues>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>variogram_kappa</ows:Identifier>
				<ows:Title>kappa (only for show)</ows:Title>
				<ows:Abstract>smoothness parameter for the Matern class of variogram models</ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer"/>
					<ows:AllowedValues>
						<ows:Value/>
					</ows:AllowedValues>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>variogram_nugget</ows:Identifier>
				<ows:Title>nugget (only for show)</ows:Title>
				<ows:Abstract>	nugget component of the variogram (this basically adds a nugget compontent to the model)</ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer"/>
					<ows:AllowedValues>
						<ows:Value/>
					</ows:AllowedValues>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>samplMethod</ows:Identifier>
				<ows:Title>samplMethod (only for show)</ows:Title>
				<ows:Abstract>Sampling Method</ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:string"/>
					<ows:AllowedValues>
						<ows:Value>put</ows:Value>
						<ows:Value>methods</ows:Value>
						<ows:Value>here</ows:Value>
					</ows:AllowedValues>
				</LiteralData>
			</Input>
		</DataInputs>
		<ProcessOutputs>
			<Output>
				<ows:Identifier>realisations</ows:Identifier>
				<ows:Title>realisations</ows:Title>
				<ows:Abstract>realisations</ows:Abstract>
				<ComplexOutput>
					<Default>
						<Format>
							<MimeType>text/xml</MimeType>
							<Schema>http://giv-uw.uni-muenster.de:8080/uts/schemas/uncertml2.0.0/Realisation.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>application/netcdf</MimeType>
						</Format>
					</Supported>
				</ComplexOutput>
			</Output>
		</ProcessOutputs>
	</ProcessDescription>
</wps:ProcessDescriptions>
