<?xml version="1.0" encoding="UTF-8"?>
<!--This example describes .... -->
<wps:ProcessDescriptions xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opengis.net/wps/1.0.0
http://schemas.opengis.net/wps/1.0.0/wpsDescribeProcess_response.xsd" xml:lang="en-US" service="WPS" version="1.0.0">
	<ProcessDescription wps:processVersion="0" storeSupported="true" statusSupported="false">
		<ows:Identifier>org.uncertweb.wps.UnivGaussianDist2Realisations</ows:Identifier>
		<ows:Title>Distribution to realisation process.</ows:Title>
		<ows:Abstract>Computes a specified number of realisations for a given gaussian distribution.</ows:Abstract>
		<ows:Metadata xlink:title="spatial"/>
		<DataInputs>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>distribution</ows:Identifier>
				<ows:Title>distribution</ows:Title>
				<ows:Abstract>distribution</ows:Abstract>
				<ComplexData>
					<Default>
						<Format>
							<MimeType>text/xml</MimeType>
							<Schema>http://giv-uw.uni-muenster.de:8080/uts/schemas/uncertml2.0.0/GaussianDistribution.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>application/netcdf</MimeType>
						</Format>
					</Supported>
				</ComplexData>
			</Input>
			
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>numbReal</ows:Identifier>
				<ows:Title>number of realisations (for real)</ows:Title>
				<ows:Abstract>number of realisations</ows:Abstract>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer"/>
					<ows:AllowedValues>
						<ows:Value/>
					</ows:AllowedValues>
					<DefaultValue>10</DefaultValue>
				</LiteralData>
			</Input>
			
		</DataInputs>
		<ProcessOutputs>
			<Output>
				<ows:Identifier>realisations</ows:Identifier>
				<ows:Title>realisations</ows:Title>
				<ows:Abstract>realisations</ows:Abstract>
				<ComplexOutput>
					<Default>
						<Format>
							<MimeType>text/xml</MimeType>
							<Schema>http://giv-uw.uni-muenster.de:8080/uts/schemas/uncertml2.0.0/Realisation.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/xml</MimeType>
							<Schema>http://v-mars.uni-muenster.de/uncertweb/schema/uncertml/uncertml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/netcdf</MimeType>
						</Format>
					</Supported>
				</ComplexOutput>
			</Output>
		</ProcessOutputs>
	</ProcessDescription>
</wps:ProcessDescriptions>
