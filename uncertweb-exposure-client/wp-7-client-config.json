{
	"mock": false,
	"visualizationUrl": "http://giv-uw.uni-muenster.de/vis/v2",
	"outputs": {
		"albatross": null,
		"syn-pop": null
	},
	"mappings": {
		"albatross": {
			"syn-pop": { 
				"export-file": "export-file",
				"export-file-bin": "export-file-bin"
			}
		}
	},
	"processes": {
		"syn-pop": {
			"url": "http://128.176.133.106:8080/albatross-syn-pop-wps/WebProcessingService",
			"mock-url": "examples/synPopProcessExampleResponse.xml",
			"id": "org.uncertweb.wps.SyntheticPopulationProcess",
			"outputs": {
				"export-file": {
					"mimeType": "application/text+xml"
				},
				"export-file-bin": {
					"mimeType": "application/text+xml"
				}
			},
			"inputs": {
				"sections": [
					{
						"title": "Parameter",
						"options": {
							"genpop-households": {
								"type": "string",
								"title": "Number of Housholds",
								"description": "Number of households created by Genpop and stored in “test.obs”.",
								"required": true,
								"default": "357"
							},
							"rwdata-households": {
								"type": "string",
								"title": "Household Activity Schedules",
								"description": "Number of household activity schedules sets created by Rwdata and stored in “test.prd”.",
								"required": true,
								"default": "0"
							},
							"postcode-areas": {
								"type": "string",
								"title": "Postcode Areas",
								"description": "Number of postcode areas (PC4).",
								"required": true,
								"default": "3987"
							},
							"zones": {
								"title": "Zones",
								"description": "Number of zones (larger areas).",
								"type": "string",
								"required": true,
								"multiple": true,
								"default": "1308"
							},
							"municipalities": {
								"type": "string",
								"title": "Municipalities",
								"description": "Number of municipalities.",
								"required": true,
								"default": "625"
							},
							"isBootstrapping": {
								"title": "Bootstrapping",
								"description": "Indicates if bootstrapping is active.",
								"type": "boolean",
								"default": false
							},
							"isModelUncertainty": {
								"title": "Model Uncertainty",
								"description": "Indicates whether model uncertainty (true) should be used or just input uncertainty (false).",
								"type": "boolean",
								"default": false
							},
							"noCases": {
								"title": "Number of Cases",
								"description": "Number of cases.",
								"type": "integer"
							},
							"noCasesNew": {
								"title": "New Number of Cases",
								"description": "Same number of cases in the new version of the file.",
								"type": "integer"
							}
						}
					}
				]
			}
		},
		"albatross": {
			"id": "org.uncertweb.wps.AlbatrossProcess",
			"url": "http://128.176.133.106:8080/albatross-wps/WebProcessingService",
			"mock-url": "examples/albatrossProcessExampleResponse.xml",
			"outputs": {
				"om_schedules": {
					"mimeType": "application/x-om-u+json",
					"asReference": true,
					"schema": "http://v-mars.uni-muenster.de/uncertweb/schema/Profiles/OM/UncertWeb_OM.xsd"
				}
			},
			"inputs": {
				"sections": [
					{
						"title": "Parameter",
						"options": {
							"genpop-households": {
								"type": "string",
								"title": "Number of Housholds",
								"description": "Number of households created by Genpop and stored in “test.obs”.",
								"required": true,
								"default": "357"
							},
							"rwdata-households": {
								"type": "string",
								"title": "Household Activity Schedules",
								"description": "Number of household activity schedules sets created by Rwdata and stored in “test.prd”.",
								"required": true,
								"default": "0"
							},
							"postcode-areas": {
								"type": "string",
								"title": "Postcode Areas",
								"description": "Number of postcode areas (PC4).",
								"required": true,
								"default": "3987"
							},
							"zones": {
								"title": "Zones",
								"description": "Number of zones (larger areas).",
								"type": "string",
								"required": true,
								"multiple": true,
								"default": "1308"
							},
							"municipalities": {
								"type": "string",
								"title": "Municipalities",
								"description": "Number of municipalities.",
								"required": true,
								"default": "625"
							},
							"isModelUncertainty": {
								"title": "Model Uncertainty",
								"description": "Indicates whether model uncertainty (true) should be used or just input uncertainty (false).",
								"type": "boolean",
								"default": false
							},
							"number-of-schedules": {
								"title": "Number of Schedules",
								"description": "Number of schedules returned in case the requested output is om_schedules.",
								"type": "integer",
								"required": false
							},
							"uncert-link": {
								"title": "uncert-link",
								"description": "link or group of links with standard deviation",
								"multiple": true,
								"required": false,
								"type": "complex",
								"properties": {
									"uncert-link-aid": {
										"type": "string",
										"required": true,
										"title": "Link ID",
										"description": "Link ID used in Albatross Model",
										"multiple":true
									},
									"uncert-link-sector": {
										"type": "integer",
										"title": "Number of links", 
										"description": "Specifies number of links",
										"required": true
									},
									"uncert-link-stddev": {
										"type": "number",
										"required": true,
										"title": "Standard Deviation",
										"description": "Standard Deviation"
									}
								}
							},
							"uncert-area": {
								"title": "uncert-area",
								"description": "area or group of areas with standard deviation for sector; sector is implemented as parameter with name attribute value 'sector'",
								"multiple": true,
								"required": false,
								"type": "complex",
								"properties": {
									"uncert-area-aid": {
										"type": "string",
										"required": true,
										"title": "Area ID",
										"description": "Area ID used in Albatross"
									},
									"uncert-area-sector": {
										"type": "choice",
										"title": "Sector", 
										"description": "the sector",
										"required": true,
										"options": {
											"1": "total across sectors",
										   	"2": "number of students/pupils in schools",
											"3": "retailing – daily goods",
											"4": "retailing non-daily goods",
											"5": "restaurants and bars",
											"6": "banks",
											"7": "population"
										}
									},
									"uncert-area-stddev": {
										"type": "number",
										"required": true,
										"title": "Standard Deviation",
										"description": "Standard Deviation"
									}
								}
							}
						}
					}
				]
			}
		}
	}
}
